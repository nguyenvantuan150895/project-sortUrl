<%- include('partials/aboveContent.ejs'); %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        DataBase Manager
        <p class="fa fa-fw fa-tripadvisor"></p> 
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Tables</a></li>
        <li class="active">Data tables</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <div class="box-header">
              
              
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <div class="register-box-body" style = "border: 1px solid #d2d6dd;   border-top: 4px solid #337ab7; border-radius: 5px; width: 100%;height: 100%; margin-left: auto; margin-right: auto">
                  <img style = "width: 40px; height: 40px" src="data:image/png;base64," alt="">
                  <h3 class="login-box-msg">Detail information</h3>
              
                  <form action= "http://127.0.0.1:3000/update" method = "POST">
                      <b>Name</b>
                      <div class="form-group has-feedback">
                      <input type="text" class="form-control"  id = "idName" name = "name" value ="<%=user.name%>" readonly>
                      <span class="glyphicon glyphicon-user form-control-feedback"></span>
                      </div>
                      <b>Age</b>
                      <div class="form-group has-feedback">
                      <input type="text" class="form-control"  id = "idAge" name = "age" value ="<%=user.age%>" readonly>
                      <span class="glyphicon glyphicon-tree-deciduous form-control-feedback"></span>
                      </div>
                      <b>Email</b>
                      <div class="form-group has-feedback">
                      <input type="text" class="form-control"  id = "idEmail" name = "email" value ="<%=user.email%>" readonly>
                      <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
                      </div>
                      <div class="row">
                      <div class="col-xs-12">
                          
                      </div>
                      <!-- /.col -->
                      <div class="col-xs-3"></div>
                      <div class="col-xs-6">
                          <a style = "float: right" class="btn btn-primary btn-md btn-block" href="/user/manager/<%=page%>">Back</a>
                      </div>
                      <div class="col-xs-3"></div>
                      
                      <!-- /.col -->
                      </div>
                  </form>
                  </div>      
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
  
    <%- include('partials/underContent.ejs'); %>