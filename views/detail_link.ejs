<%- include('partials/aboveContent.ejs'); %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        DataBase Manager
        <p class="fa fa-fw fa-tripadvisor"></p> 
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Tables</a></li>
        <li class="active">Data tables</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <div class="box-header">
              
              
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <div class="register-box-body" style = "border: 1px solid #d2d6dd;   border-top: 4px solid #337ab7; border-radius: 5px; width: 100%;height: 100%; margin-left: auto; margin-right: auto">
                  <img style = "width: 40px; height: 40px" src="/iconDetailLink.png" alt=""> 
                  <h3 class="login-box-msg">Detail information</h3>
              
                  <form action= "#" method = "POST">
                      <b>Random</b>
                      <div class="form-group has-feedback">
                      <input type="text" class="form-control"  id = "idUsername" name = "random" value ="<%=url.random%>" readonly>
                      <span class="glyphicon glyphicon-random form-control-feedback"></span>
                      </div>
                      <b>md5</b>
                      <div class="form-group has-feedback">
                      <input type="text" class="form-control"  id = "idPassword" name = "md5" value ="<%=url.md5%>" readonly>
                      <span class="fa fa-filter form-control-feedback"></span>
                      </div>
                      <b>Url Original</b>
                      <div class="form-group has-feedback">
                      <input type="text" class="form-control"  id = "idEmail" name = "oldUrl" value ="<%=url.oldUrl%>" readonly>
                      <span class="fa fa-link form-control-feedback"></span>
                      </div>
                      <b>Url Shorten</b>
                      <div class="form-group has-feedback">
                      <input type="text" class="form-control"  id = "idEmail" name = "newUrl" value ="<%=url.newUrl%>" readonly>
                      <span class="fa fa-link form-control-feedback"></span>
                      </div>
                      <b>User</b>
                      <div class="form-group has-feedback">
                      <input type="text" class="form-control"  id = "idEmail" name = "user" value ="<%=url.user%>" readonly>
                      <span class="glyphicon glyphicon-user form-control-feedback"></span>
                      </div>
                      <div class="row">
                      <div class="col-xs-12">
                          
                      </div>
                      <!-- /.col -->
                      <div class="col-xs-3"></div>
                      <div class="col-xs-6">
                          <a style = "float: right" class="btn btn-primary btn-md btn-block" href="/admin/manager/link/<%=page%>">Back</a>
                      </div>
                      <div class="col-xs-3"></div>
                      
                      <!-- /.col -->
                      </div>
                  </form>
                  </div>      
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
  
    <%- include('partials/underContent.ejs'); %>